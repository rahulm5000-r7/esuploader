<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" name="baeldung" packages="">
   <Appenders>
      <Console name="STDOUT" target="SYSTEM_OUT">
         <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
      </Console>
   </Appenders>
   <Loggers>
      <Root level="info">
         <AppenderRef ref="STDOUT"/>
      </Root>
   </Loggers>
</Configuration>